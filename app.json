{
  "expo": {
    "name": "Дабравесце",
    "slug": "dabravesce",
    "version": "Two tab layout introduced",
    "runtimeVersion": "41",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "dwapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "by.dabravesce"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#fcfaeb"
      },
      "package": "by.dabravesce",
      "versionCode": 41
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "backgroundColor": "#fcfaeb",
          "resizeMode": "native",
          "imageWidth": 480
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "9ed2d8a4-b203-4b54-8fc8-4ece25041f0b"
      }
    }
  }
}
